{
  "$schema": "https://json.schemastore.org/claude-code-settings.json",
  "permissions": {
    "allow": [
      "Bash(npm install:*)",
      "Bash(cp:*)",
      "Bash(npm run build:*)",
      "Bash(npx tsc:*)",
      "Bash(rm:*)",
      "Bash(npx next lint:*)",
